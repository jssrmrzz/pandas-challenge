    "# percentage calculated\n",
    "pctMVF = (mvf / totalPlayers * 100)\n",
    "pctMVF\n",
    "# Summary Table expressing Total Count and Percentage\n",
    "mvfSummT = pd.DataFrame({'Total Count': mvf, 'Percent of Players': pctMVF})\n",
    "mvfSummT"
